<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/space_12_dp" />
            <gradient
                android:angle="270"
                android:centerColor="#ffEEEEEE"
                android:centerY="2"
                android:endColor="#ffEEEEEE"
                android:startColor="#ffEEEEEE" />
            <stroke
                android:width="@dimen/space_2_dp"
                android:color="@color/color979797" />
        </shape>
    </item>
    <!--第一进度条-->
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/space_12_dp" />
                <gradient
                    android:angle="270"
                    android:centerColor="#ffd47a3e"
                    android:centerY="2"
                    android:endColor="#ffd47a3e"
                    android:startColor="#ffd47a3e" />
                <stroke
                    android:width="@dimen/space_2_dp"
                    android:color="@color/color979797" />
            </shape>
        </clip>
    </item>
</layer-list>
